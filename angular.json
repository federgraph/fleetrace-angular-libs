{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "scoring": {
      "projectType": "library",
      "root": "projects/scoring",
      "sourceRoot": "projects/scoring/src",
      "prefix": "scoring",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/scoring/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/scoring/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/scoring/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/scoring/src/**/*.ts",
              "projects/scoring/src/**/*.html"
            ]
          }
        }
      }
    },
    "fleetrace": {
      "projectType": "library",
      "root": "projects/fleetrace",
      "sourceRoot": "projects/fleetrace/src",
      "prefix": "fr",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/fleetrace/tsconfig.lib.prod.json"
        },
            "development": {
              "tsConfig": "projects/fleetrace/tsconfig.lib.json"
          }
        },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/fleetrace/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/fleetrace/src/**/*.ts",
              "projects/fleetrace/src/**/*.html"
            ]
          }
        }
      }
    },
    "fr-local": {
      "projectType": "library",
      "root": "projects/fr-local",
      "sourceRoot": "projects/fr-local/src",
      "prefix": "fr",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/fr-local/tsconfig.lib.prod.json"
        },
            "development": {
              "tsConfig": "projects/fr-local/tsconfig.lib.json"
          }
        },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/fr-local/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/fr-local/src/**/*.ts",
              "projects/fr-local/src/**/*.html"
            ]
          }
        }
      }
    },
    "fr-remote": {
      "projectType": "library",
      "root": "projects/fr-remote",
      "sourceRoot": "projects/fr-remote/src",
      "prefix": "fr",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/fr-remote/tsconfig.lib.prod.json"
          },
            "development": {
              "tsConfig": "projects/fr-remote/tsconfig.lib.json"
          }
        },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/fr-remote/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/fr-remote/src/**/*.ts",
              "projects/fr-remote/src/**/*.html"
            ]
          }
        }
      }
    },
    "fr01": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/fr01",
      "sourceRoot": "projects/fr01/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/fr01/src/main.ts",
            "tsConfig": "projects/fr01/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/fr01/public"
              }
            ],
            "styles": [
              "projects/fr01/src/custom-theme.scss",
              "projects/fr01/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "fr01:build:production"
            },
            "development": {
              "buildTarget": "fr01:build:development"
          }
        },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/fr01/src/**/*.ts",
              "projects/fr01/src/**/*.html"
            ]
          }
        }
      }
    }
 }
}